

import time
import os
import json

file1 = open('/var/log/suricata/eve.json')



while True:
	where = file1.tell()
	line = file1.readline()

	if not line:
		file1.seek(where)
		
	else:
		try:
			parsed = json.loads(line)

			if 'proto' in parsed:
				if (parsed['proto'] == u'ICMP'):
					print(parsed["timestamp"])
		except:
			continue	
		
	
